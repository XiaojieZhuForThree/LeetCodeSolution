package problems;

public class Question1392 {
	class Solution {
		public String longestPrefix(String s) {
			int n = s.length();
			int[] lps = new int[n];
			int i = 0, j = 1;
			while (j < n) {
				if (s.charAt(j) == s.charAt(i)) {
					lps[j] = i + 1;
					j++;
					i++;
				} else {
					if (i == 0) {
						j++;
					} else {
						i = lps[i - 1];
					}
				}
			}
			return s.substring(0, lps[n - 1]);
		}
	}
}
