class Solution {
    public boolean isSubsequence(String s, String t) {
        int index = 0;
        StringBuilder sb = new StringBuilder();
        for (char i : s.toCharArray()){
            while (index < t.length()) {
                if (t.charAt(index) == i) {
                    sb.append(i);
                    index++;
                    break;
                } else {
                    index++;
                }
            }
        }
        return sb.toString().equals(s);
    }
}
