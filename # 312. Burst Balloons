class Solution {
	public int maxCoins(int[] nums) {
        int n = nums.length;
                    
        int[] a = new int[n + 2];
        a[0] = 1; a[a.length - 1] = 1;
        for (int i = 0; i<n; i++) {
            a[i+1] = nums[i];
        }      
        
        int[][] dp = new int[a.length][a.length];
        
        for (int len = 0; len < n; len++) {
            for (int i = 1; i + len <=n; i++) {
                int j = i + len;
                for (int k = i; k <= j; k++) {
                    dp[i][j] = Math.max(dp[i][j], dp[i][k-1] + dp[k+1][j] + a[k]*a[i-1]*a[j+1]);
                }
            }
        }
        
        return dp[1][n];
    }
}
