class Solution {
    public int deleteAndEarn(int[] nums) {
        
        int[] dp = new int[10002];
        int max = 0;
        for (int i : nums) {
            dp[i]++;
            max = Math.max(max, i);
        }
        for (int i = 2; i <= max; i++) {
            dp[i] = Math.max(dp[i - 1], dp[i - 2] + i * dp[i]);
        }
        return dp[max];
    }
}
