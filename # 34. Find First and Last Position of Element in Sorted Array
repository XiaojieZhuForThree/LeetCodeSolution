class Solution {
	public int[] searchRange(int[] nums, int target) {
		int l = 0, r = nums.length;
		while (l < r) {
			int mid = (l + r) / 2;
			if (nums[mid] == target) {
				int lt = mid, rt = mid;
				while (lt >= 0 && nums[lt] == target) {
					lt--;
				}
				while (rt < nums.length && nums[rt] == target) {
					rt++;
				}
				return new int[] { lt + 1, rt - 1 };
			} else if (nums[mid] < target) {
				l = mid + 1;
			} else {
				r = mid;
			}
		}
		return new int[] { -1, -1 };
	}
}
