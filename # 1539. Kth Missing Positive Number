package com.leetcode.problems;

public class Question1539 {
	class Solution {
		public int findKthPositive(int[] arr, int k) {
			int[] nums = new int[10001];
			for (int i : arr) {
				nums[i] = 1;
			}
			int cnt = 0;
			for (int i = 1; i <= nums.length; i++) {
				if (nums[i] == 0) {
					cnt++;
					if (cnt == k) {
						return i;
					}
				}
			}
			return -1;
		}
	}
}
