package com.leetcode.problems;

import com.leetcode.api.ListNode;

public class Question21 {
	class Solution {
		public ListNode mergeTwoLists(ListNode l1, ListNode l2) {
			ListNode node = new ListNode(-1);
			ListNode trav = node;
			while (l1 != null && l2 != null) {
				if (l1.val < l2.val) {
					trav.next = l1;
					l1 = l1.next;
				} else {
					trav.next = l2;
					l2 = l2.next;
				}
				trav = trav.next;
			}
			if (l1 != null) {
				trav.next = l1;
			} else {
				trav.next = l2;
			}
			return node.next;
		}
	}
}
