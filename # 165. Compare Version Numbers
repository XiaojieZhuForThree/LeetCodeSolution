package com.leetcode.problems;

public class Question165 {
	class Solution {
		public int compareVersion(String version1, String version2) {
			String[] partOfVersion1 = version1.split("\\.");
			String[] partOfVersion2 = version2.split("\\.");
			int len = Math.max(partOfVersion1.length, partOfVersion2.length);
			for (int i = 0; i < len; i++) {
				int version1LevelI = i >= partOfVersion1.length ? 0 : Integer.parseInt(partOfVersion1[i]);
				int version2LevelI = i >= partOfVersion2.length ? 0 : Integer.parseInt(partOfVersion2[i]);
				if (Integer.compare(version1LevelI, version2LevelI) == 0) {
					continue;
				}
				return Integer.compare(version1LevelI, version2LevelI);
			}
			return 0;
		}
	}
}
