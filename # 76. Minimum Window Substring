class Solution {
    public String minWindow(String s, String t) {
    	String ans = s;
        Set<Character> set = new HashSet<>();
        for (char i : t.toCharArray()) {
        	set.add(i);
        }
        int left = 0, right = 0;
        while (left < s.length()) {
        	if (set.contains(s.charAt(left))){
        		Set<Character> newset = new HashSet<>(set);
        		newset.remove(s.charAt(left));
        		right = left+1;
        		while (right < s.length() && !set.isEmpty()) {
        			if (newset.contains(s.charAt(right))) {
        				newset.remove(s.charAt(right));        				
        			}
        			right++;
        		}
        		if (!newset.isEmpty()) {
        			return "";
        		}
        		else if (right - left + 1 < ans.length()){
        			ans = s.substring(left, right + 1);
        		}
                left = right+1;
        	}
            else {
                left++;
            }
        }
        return ans;
    }
}
