import java.util.*;

//Definition for a binary tree node.
class TreeNode {
	int val;
	TreeNode left;
	TreeNode right;

	TreeNode(int x) {
		val = x;
	}
}

class Solution {
	public List<Integer> rightSideView(TreeNode root) {
		List<Integer> ans = new ArrayList<>();
		Deque<TreeNode> deque = new ArrayDeque<>();
		if (root == null)
			return ans;
		deque.addFirst(root);
		while (!deque.isEmpty()) {
			ans.add(deque.peekLast().val);
			int length = deque.size();
			for (int i = 0; i < length; i++) {
				TreeNode one = deque.pollFirst();
				if (one.left != null)
					deque.addLast(one.left);
				if (one.right != null)
					deque.addLast(one.right);
			}
		}
		return ans;
	}
}
