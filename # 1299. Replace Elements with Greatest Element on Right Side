package problems;

public class Question1299 {
	class Solution {
		public int[] replaceElements(int[] arr) {
			int max = arr[arr.length - 1];
			for (int i = arr.length - 2; i >= 0; i--) {
				int rep = max;
				max = Math.max(arr[i], max);
				arr[i] = rep;
			}
			arr[arr.length - 1] = -1;
			return arr;
		}
	}
}
