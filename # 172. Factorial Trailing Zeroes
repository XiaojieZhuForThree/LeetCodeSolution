class Solution {
    public int trailingZeroes(int n) {
        int ans = 0, num = fac(n);
        while (num != 0 && num%10 == 0) {
        	ans++;
        	num /= 10;
        }
        return ans;
    }
    private int fac(int i) {
    	if (i == 0 || i == 1) {
    		return 1;
    	}
    	return i*fac(i-1);
    }
}
