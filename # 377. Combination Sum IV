class Solution {
    public int combinationSum4(int[] nums, int target) {
        if (nums == null || nums.length == 0 || target < 0) {
            return 0;
        }
        int[] dp = new int[target+1];
        for (int i : nums) {
            if (i <= target) {
                dp[i] = 1;                
            }
        }
        for (int i = 1; i <= target; i++) {
            for (int j : nums) {
                if (i - j >= 0) {
                    dp[i] += dp[i-j];
                }
            }
        }
        return dp[target];
    }
}
