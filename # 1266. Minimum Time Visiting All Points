
public class Question1266 {
	class Solution {
		public int minTimeToVisitAllPoints(int[][] points) {
			int ans = 0;
			for (int i = 1; i < points.length; i++) {
				int[] p1 = points[i - 1];
				int[] p2 = points[i];
				int dis1 = Math.abs(p1[0] - p2[0]), dis2 = Math.abs(p1[1] - p2[1]);
				ans += Math.max(dis1, dis2);
			}
			return ans;
		}
	}
}
