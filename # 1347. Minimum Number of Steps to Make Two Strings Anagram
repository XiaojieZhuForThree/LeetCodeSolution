package problems;

public class Question1347 {
	class Solution {
		public int minSteps(String s, String t) {
			int chars[] = new int[26];
			for (char c : s.toCharArray()) {
				chars[c - 'a']++;
			}
			int ans = t.length();
			for (char c : t.toCharArray()) {
				if (chars[c - 'a'] > 0) {
					ans--;
					chars[c - 'a']--;
				}
			}
			return ans;
		}
	}
}
