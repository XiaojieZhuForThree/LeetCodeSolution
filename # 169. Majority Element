class Solution {
    public int majorityElement(int[] nums) {
        int ans = nums[0];
        int count = 0;
        for (int i : nums){
            if (count == 0){
                ans = i;
            }
            count += (ans == i) ? 1: -1;
        }
        return ans;
    }
}


class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        cand = -1
        freq = 0
        for i in nums:
            if freq == 0:
                cand = i
                freq = 1
            elif i == cand:
                freq += 1
            else:
                freq -= 1
        return cand
