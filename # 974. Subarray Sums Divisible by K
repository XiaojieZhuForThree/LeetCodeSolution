class Solution {
    public int subarraysDivByK(int[] A, int K) {
        int[] count = new int[K];
        count[0] = 1;
        int prefix = 0, ans = 0;
        for (int i : A) {
            prefix = (prefix + i % K + K) % K;
            ans += count[prefix]++;
        }
        return ans;
    }
}
