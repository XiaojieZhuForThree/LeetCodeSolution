package problems;

public class Question1262 {
	class Solution {
		public int maxSumDivThree(int[] nums) {
			int[][] dp = new int[nums.length + 1][3];
			for (int i = 1; i <= nums.length; i++) {
				int num = nums[i - 1];
				for (int j = 0; j < 3; j++) {
					dp[i][j] = dp[i - 1][j];
				}
				for (int j = 0; j < 3; j++) {
					int cur = num + dp[i - 1][j];
					dp[i][cur % 3] = Math.max(dp[i][cur % 3], cur);
				}
			}
			return dp[nums.length][0];
		}
	}
}
