package com.leetcode.problems;

public class Question389 {
	class Solution1 {
		public char findTheDifference(String s, String t) {
			int[] chars = new int[26];
			for (char c : s.toCharArray()) {
				chars[c - 'a']++;
			}
			for (char c : t.toCharArray()) {
				chars[c - 'a']--;
				if (chars[c - 'a'] < 0) {
					return c;
				}
			}
			return 'a';
		}
	}

	class Solution2 {
		public char findTheDifference(String s, String t) {
			char ans = 0;
			int i = 0;
			for (; i < t.length() - 1; i++) {
				ans ^= s.charAt(i);
				ans ^= t.charAt(i);
			}
			ans ^= t.charAt(i);
			return ans;
		}
	}
}
