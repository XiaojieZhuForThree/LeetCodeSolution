class Solution {
    Map<Integer, Integer> map = new HashMap<>();
    public int numTrees(int n) {
        if ( n < 1 ) return 0;
        if (n == 1) return 1;
        return helper(n);
    }
    private int helper(int n) {
        if (map.containsKey(n)) return map.get(n);
        if (n == 0 || n == 1){
            return 1;
        }
        int ans = 0;
        for (int i = 1; i <= n; i++){
            ans += helper(i-1) * helper(n-i);
        }
        map.put(n, ans);
        return ans;
    }
}
