class Solution {
    public int minDominoRotations(int[] A, int[] B) {
        int ans1 = Math.min(helper(A, B, A[0]), helper(B, A, A[0]));
        int ans2 = Math.min(helper(A, B, B[0]), helper(B, A, B[0]));
        int ans = Math.min(ans1, ans2);
        if (ans == Integer.MAX_VALUE) {
            return -1;
        }
        return ans;
    }
    private int helper(int[] A, int[] B, int target) {
        int ans = 0;
        for (int i = 0; i < A.length; i++) {
            if (A[i] == target) {
                continue;
            } else if (B[i] == target) {
                ans++;
            } else {
                return Integer.MAX_VALUE;
            }
        }
        return ans;
    }
}
