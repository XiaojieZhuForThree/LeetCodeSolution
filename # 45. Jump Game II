class Solution {
    public int jump(int[] nums) {
        int[] anss = new int[nums.length];
        Arrays.fill(anss, Integer.MAX_VALUE);
        anss[0] = 0;
        int remaining = nums[0];
        for (int i = 0; i < nums.length; i++) {
            for (int j = i+1; j <= Math.min(i + nums[i], nums.length-1); j++){
                anss[j] = Math.min(anss[j], anss[i] + 1);
            }
        }
        return anss[nums.length - 1];
    }
}
