package problems;

import java.util.ArrayList;
import java.util.List;

public class Question1291 {
	class Solution {
		public List<Integer> sequentialDigits(int low, int high) {
			List<Integer> ans = new ArrayList<>();
			int test = 1, lB = 0, hB = 0;
			while (test <= low) {
				test *= 10;
				lB++;
				hB++;
			}
			while (test <= high) {
				test *= 10;
				hB++;
			}
			for (; lB <= hB; lB++) {
				ans.addAll(getSeq(lB, low, high));
			}
			return ans;
		}

		private List<Integer> getSeq(int bound, int low, int high) {
			List<Integer> ans = new ArrayList<>();
			for (int i = 1; i < 10; i++) {
				int j = i, dig = 0, val = 0;
				while (dig < bound && j < 10) {
					val = val * 10 + j;
					dig++;
					j++;
				}
				if (dig == bound && low <= val && val <= high) {
					ans.add(val);
				}
			}
			return ans;
		}
	}
}
