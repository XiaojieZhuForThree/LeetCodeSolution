package problems;

import java.util.ArrayList;
import java.util.List;

public class Question1018 {
	class Solution {
		public List<Boolean> prefixesDivBy5(int[] A) {
			List<Boolean> ans = new ArrayList<>();
			int val = 0;
			for (int i = 0; i < A.length; i++) {
				val *= 2;
				val += A[i];
				ans.add(val % 5 == 0);
				val %= 5;
			}
			return ans;
		}
	}
}
