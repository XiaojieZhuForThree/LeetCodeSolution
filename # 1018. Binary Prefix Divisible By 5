package problems;

import java.util.ArrayList;
import java.util.List;

public class Question1018 {
	class Solution {
		public List<Boolean> prefixesDivBy5(int[] A) {
			List<Boolean> ans = new ArrayList<>();
			int val = 0;
			for (int i = 0; i < A.length; i++) {
				val = (val * 2 + A[i]) % 5;
				ans.add(val == 0);
			}
			return ans;
		}
	}
}
