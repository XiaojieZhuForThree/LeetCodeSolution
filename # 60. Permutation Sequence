class Solution {
    public String getPermutation(int n, int k) {
        
    }
    private int helper(int n, int k) {
        int div = getFac(n - 1);
        int seq = k / div;
        int remainder = k % div;
    }
    private int getFac(int n) {
        if (n == 0) return 1;
        return n * getFac(n-1);
    }
}
