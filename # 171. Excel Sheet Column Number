class Solution {
    public int titleToNumber(String s) {
        if (s == null || s.length() == 0){
            return 0;
        }
        else if (s.length() == 1){
            return s.charAt(0) - 'A' + 1;
        }
        else {
            return (s.charAt(0) - 'A' + 1) * (int)Math.pow(26, s.length() - 1) + titleToNumber(s.substring(1));
        }
    }
}
