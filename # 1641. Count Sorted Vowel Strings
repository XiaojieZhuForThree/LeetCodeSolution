package com.leetcode.problems;

import java.util.Arrays;

public class Question1641 {
	class Solution {
		public int countVowelStrings(int n) {
			if (n == 1) {
				return 5;
			}
			if (n == 2) {
				return 15;
			}
			int[] occurs = new int[6];
			Arrays.fill(occurs, 1);
			for (int i = 3; i <= n; i++) {
				int[] next = new int[6];
				next[5] = 1;
				int accu = 1;
				for (int j = 4; j >= 1; j--) {
					next[j] = occurs[j];
					next[j] += accu;
					accu += occurs[j];
				}
				occurs = next;
			}
			int sum = 0;
			for (int i = 1; i <= 5; i++) {
				sum += i * occurs[i];
			}
			return sum;
		}
	}
}
