package problems;

public class Question938 {
	class Solution {
		int ans = 0;

		public int rangeSumBST(TreeNode root, int L, int R) {
			dfs(root, L, R);
			return ans;
		}

		private void dfs(TreeNode root, int L, int R) {
			if (root == null) {
				return;
			}
			if (root.val <= R && root.val >= L) {
				ans += root.val;
			}
			if (root.val <= R) {
				dfs(root.right, L, R);
			}
			if (root.val >= L) {
				dfs(root.left, L, R);
			}
		}
	}
}
