class Solution {
    public int integerBreak(int n) {
        int[] prevs = new int[n+1];
        prevs[1] = 1;
        for (int i = 2; i < n+1; i++) {
            int val = 0;
            for (int j = 1; j <= i/2; j++) {
                val = Math.max(Math.max(j, prevs[j]) * Math.max(i-j, prevs[i-j]), val);
            }
            prevs[i] = val;
        }
        return prevs[n];
    }
}
