package com.leetcode.problems;

import com.leetcode.api.ListNode;

public class Question147 {
	class Solution {
		public ListNode insertionSortList(ListNode head) {
			if (head == null || head.next == null) {
				return head;
			}
			ListNode top = new ListNode(0), trav = head;
			top.next = head;
			while (trav != null && trav.next != null) {
				ListNode target = trav.next, nextNode = trav.next.next;
				trav.next = nextNode;
				ListNode dummy = top;
				boolean inserted = false;
				while (dummy != trav) {
					ListNode compare = dummy.next;
					if (compare.val > target.val) {
						dummy.next = target;
						target.next = compare;
						inserted = true;
						break;
					} else {
						dummy = dummy.next;
					}
				}
				if (!inserted) {
					trav.next = target;
					target.next = nextNode;
					trav = trav.next;
				}
			}
			return top.next;
		}
	}
}
