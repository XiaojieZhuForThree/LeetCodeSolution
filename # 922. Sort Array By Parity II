package problems;

public class Question922 {
	class Solution {
		public int[] sortArrayByParityII(int[] A) {
			int[] sortA = new int[A.length];
			int even = 0, odd = 1;
			for (int i : A) {
				if (i % 2 == 0) {
					sortA[even] = i;
					even += 2;
				} else {
					sortA[odd] = i;
					odd += 2;
				}
			}
			return sortA;
		}
	}
}
