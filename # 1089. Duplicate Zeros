import java.util.LinkedList;
import java.util.Queue;

public class Question1089 {
	class Solution {
		public void duplicateZeros(int[] arr) {
			Queue<Integer> stack = new LinkedList<>();
			for (int i : arr) {
				stack.add(i);
			}
			int i = 0;
			while (i < arr.length) {
				int j = stack.poll();
				if (j == 0) {
					arr[i++] = 0;
					if (i < arr.length) {
						arr[i++] = 0;
					}
				} else {
					arr[i++] = j;
				}
			}
			return;
		}
	}
}
