class Solution {
    public int numDecodings(String s) {
        if(s == null || s.length() == 0)return 0;
        int[] dp = new int[s.length()+1];
        dp[0]=1;
        for (int i=0;i<s.length();i++) {
            int cur= s.charAt(i)-'0';
            if (cur > 0)dp[i+1] = dp[i];
            if (i > 0)cur += (s.charAt(i-1)-'0')*10;
            if(cur >= 10 && cur <= 26) {
                dp[i+1] += dp[i-1];
            }  
        }
        return dp[s.length()];
    }
}# 
